<center>
  <h1>RoomRegistration</h1>
  <form action="/boards" method="POST">
    <input type="text" name="board_name">
    <input type="submit">
  </form>
  <ul style="list-style:none;">
    <% @rooms.each do |room| %>
      <li><a href="/boards/<%= room.name %>"><%= room.screen_name  %></a></li>
    <% end %>
  </ul>
</center>
